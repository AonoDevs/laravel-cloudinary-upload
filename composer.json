{
    "name": "aonodevs/laravel-cloudinary-upload",
    "description": "Package to upload images and videos on cloudinary and save their url into model's database",
    "keywords": [
        "Aono",
        "laravel",
        "laravel-cloudinary-upload"
    ],
    "homepage": "https://github.com/aonodevs/laravel-cloudinary-upload",
    "license": "MIT",
    "authors": [
        {
            "name": "Roriwars",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "cloudinary/cloudinary_php": "^2.11",
        "intervention/image": "^2.7",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "AonoDevs\\LaravelCloudinaryUpload\\": "src/",
            "AonoDevs\\LaravelCloudinaryUpload\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AonoDevs\\LaravelCloudinaryUpload\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AonoDevs\\LaravelCloudinaryUpload\\LaravelCloudinaryUploadServiceProvider"
            ],
            "aliases": {
                "LaravelCloudinaryUpload": "AonoDevs\\LaravelCloudinaryUpload\\Facades\\LaravelCloudinaryUpload"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
